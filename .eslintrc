extends:
  - eslint:recommended
  - angular
rules:
  comma-dangle:
  - 2
  - never
  jsx-quotes:
  - 2
  - prefer-single
  key-spacing:
  - 0
  max-len:
  - 1
  - 360
  no-bitwise:
  - 2
  no-console:
  - 0
  no-redeclare:
  - 2
  no-trailing-spaces:
  - 2
  no-extra-semi:
  - 2
  no-undef:
  - 2
  no-underscore-dangle:
  - 0
  no-unused-vars:
  - 2
  brace-style:
  - 2
  - 1tbs
  - allowSingleLine: true
  no-use-before-define:
  - 0
  quotes:
  - 2
  - single
  - allowTemplateLiterals: true
    avoidEscape: true
  semi:
  - 2
  - always
  keyword-spacing:
  - 2
  - before: true
    after: true
  space-before-blocks: 2
  space-before-function-paren:
  - 2
  - never
  space-in-parens:
  - 2
  - never
  strict:
  - 0
  valid-typeof: 2